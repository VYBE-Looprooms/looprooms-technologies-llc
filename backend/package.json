{
    "name": "vybe-looprooms-backend",
    "version": "1.0.0",
    "description": "VYBE LOOPROOMS™ Backend API for email notifications and waitlist management",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "db:migrate": "sequelize db:migrate",
        "db:migrate:undo": "sequelize db:migrate:undo",
        "db:seed": "sequelize db:seed:all",
        "db:seed:undo": "sequelize db:seed:undo:all",
        "db:reset": "npm run db:seed:undo && npm run db:migrate:undo && npm run db:migrate && npm run db:seed"
    },
    "keywords": [
        "vybe",
        "looprooms",
        "email",
        "api",
        "waitlist"
    ],
    "author": "VYBE LOOPROOMS™",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.2.1",
        "helmet": "^7.1.0",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.9.7",
        "pg": "^8.16.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.7",
        "sequelize-cli": "^6.6.3"
    },
    "devDependencies": {
        "nodemon": "^3.0.2"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
